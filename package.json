{
	"name": "divide-lines",
	"version": "1.0.3",
	"description": "Divide the lines of a string in a lines object",
	"license": "MIT",
	"type": "module",
	"exports": {
		"types": "./dist/index.d.ts",
		"default": "./dist/index.js"
	},
	"engines": {
		"node": ">=18"
	},
	"files": [
		"dist"
	],
	"types": "dist/index.d.ts",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/forresst/divide-lines.git"
	},
	"bugs": {
		"url": "https://github.com/forresst/divide-lines/issues"
	},
	"homepage": "https://github.com/forresst/divide-lines#readme",
	"author": {
		"name": "St√©phane Reynaud",
		"url": "https://github.com/forresst"
	},
	"scripts": {
		"test": "xo && tsc && c8 ava",
		"build": "del-cli dist && tsc",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"divide",
		"string",
		"lines",
		"line",
		"split",
		"array"
	],
	"devDependencies": {
		"@sindresorhus/tsconfig": "^7.0.0",
		"ava": "^6.2.0",
		"c8": "^10.1.3",
		"del-cli": "^6.0.0",
		"typescript": "^5.7.2",
		"xo": "^0.60.0"
	},
	"c8": {
		"reporter": [
			"lcov",
			"text"
		],
		"extensions": [
			"ts"
		]
	},
	"xo": {
		"parserOptions": {
			"project": "./test/tsconfig.json"
		}
	},
	"ava": {
		"extensions": {
			"ts": "module"
		},
		"nodeArguments": [
			"--import=ts-blank-space/register"
		]
	},
	"dependencies": {
		"ts-blank-space": "^0.6.0"
	}
}
